// Setup
const recordCollection = {
  2548: {
    albumTitle: 'Slippery When Wet',
    artist: 'Bon Jovi',
    tracks: ['Let It Rock', 'You Give Love a Bad Name']
  },
  2468: {
    albumTitle: '1999',
    artist: 'Prince',
    tracks: ['1999', 'Little Red Corvette']
  },
  1245: {
    artist: 'Robert Palmer',
    tracks: []
  },
  5439: {
    albumTitle: 'ABBA Gold'
  }
};

// Only change code below this line
function updateRecords(records, id, prop, value) {
  console.log("---before---");
  console.log(recordCollection[id]);

  if (value == "") {
    console.log("value = empty");
    delete recordCollection[id][prop];

  } else if (prop == "tracks") {
    console.log("value is tracks")
    if (recordCollection[id].hasOwnProperty(prop)) {
      console.log("tracks property exists");
      recordCollection[id][prop].push(value);
    } else { 
      console.log("tracks property not found");
      recordCollection[id][prop] = [];
      recordCollection[id][prop].push(value);
    } 
  } else {
    console.log("not tracks");
    recordCollection[id][prop] = value;
  }
  
  console.log("---after---")
  console.log(recordCollection[id]);
  records = recordCollection[id][prop];
  return records;
}


console.log(updateRecords(recordCollection, 1245, 'albumTitle', 'Riptide'));
